<?xml version="1.0" encoding="UTF-8"?>
<component type="desktop-application">
  <id>io.github.spacingbat3.webcord</id>

  <name>WebCord</name>
  <summary>A web-based Discord and Fosscord client made with Electron</summary>

  <metadata_license>CC0-1.0</metadata_license>
  <project_license>MIT</project_license>

  <developer_name>Contributors of WebCord</developer_name>

  <supports>
    <control>pointing</control>
    <control>keyboard</control>
    <control>touch</control>
  </supports>

  <description>
    <p>
      This flatpak is not supported by webcord directly! Please open issues on the flathub repository first!
    </p>
    <p>
      A web-based Discord and Fosscord client made with Electron
    </p>
  </description>


  <screenshots>
    <screenshot type="default">
      <caption>About page</caption>
      <image>https://i.postimg.cc/sDpv6zcM/image.png</image>
    </screenshot>
  </screenshots>

  <launchable type="desktop-id">io.github.spacingbat3.webcord.desktop</launchable>

  <content_rating type="oars-1.1">
    <content_attribute id="social-chat">intense</content_attribute>
    <content_attribute id="social-info">intense</content_attribute>
    <content_attribute id="social-audio">intense</content_attribute>
    <content_attribute id="social-contacts">intense</content_attribute>
    <content_attribute id="money-purchasing">intense</content_attribute>
  </content_rating>

